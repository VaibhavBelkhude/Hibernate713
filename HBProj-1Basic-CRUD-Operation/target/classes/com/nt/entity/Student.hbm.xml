<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
 
 <class name="com.nt.entity.Student" table="STUDENT">
 <id name="sid" type="int" column="SID">
 <generator class="increment"/>
 </id>
 <property name="Sname" type="string" column="SNAME" length="20" unique="true" not-null="true"/>
 <property name="Saddr" type="string" column="Saddr" length="20" not-null="true"/>
 <property name="Fees" type="float"/>
 

 </class>
 
 </hibernate-mapping>
        