<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	<hibernate-mapping>
	<class name="com.nt.model.Employee" table="Employeee_HQl">
	<id name="no" column="EID"/>
	<property name="Ename" column="ENAME" length="20" not-null="true"/>
	<property name="Eaddr" column="EADDR" length="20"/>
	<property name="Esal" column="ESAL" />
	</class>
	</hibernate-mapping>